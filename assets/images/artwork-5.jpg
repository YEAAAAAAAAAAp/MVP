<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg5" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#56ab2f;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#a8e6cf;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#88d8a3;stop-opacity:1" />
    </linearGradient>
    <radialGradient id="leaf" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#7ed321;stop-opacity:0.8" />
      <stop offset="100%" style="stop-color:#417505;stop-opacity:0.6" />
    </radialGradient>
  </defs>
  
  <!-- Background -->
  <rect width="400" height="300" fill="url(#bg5)"/>
  
  <!-- Tree trunk -->
  <rect x="190" y="180" width="20" height="80" fill="#8b4513" opacity="0.8" rx="10"/>
  
  <!-- Tree crown -->
  <circle cx="200" cy="160" r="50" fill="url(#leaf)" opacity="0.7"/>
  <circle cx="180" cy="140" r="35" fill="url(#leaf)" opacity="0.6"/>
  <circle cx="220" cy="135" r="40" fill="url(#leaf)" opacity="0.8"/>
  
  <!-- Leaves falling -->
  <ellipse cx="120" cy="100" rx="6" ry="10" fill="#7ed321" opacity="0.8" transform="rotate(15 120 100)"/>
  <ellipse cx="280" cy="120" rx="6" ry="10" fill="#417505" opacity="0.7" transform="rotate(-25 280 120)"/>
  <ellipse cx="150" cy="200" rx="6" ry="10" fill="#9acd32" opacity="0.6" transform="rotate(45 150 200)"/>
  <ellipse cx="320" cy="180" rx="6" ry="10" fill="#7ed321" opacity="0.9" transform="rotate(-10 320 180)"/>
  
  <!-- Mountains in background -->
  <polygon points="50,220 120,150 190,220" fill="#2c5530" opacity="0.4"/>
  <polygon points="180,220 250,140 320,220" fill="#2c5530" opacity="0.3"/>
  <polygon points="280,220 350,160 400,220" fill="#2c5530" opacity="0.5"/>
  
  <!-- Water/river -->
  <path d="M0 240 Q100 230 200 240 T400 240 L400 300 L0 300 Z" fill="#4a90e2" opacity="0.6"/>
  
  <!-- Ripples -->
  <ellipse cx="150" cy="245" rx="15" ry="3" fill="none" stroke="#ffffff" stroke-width="1" opacity="0.5"/>
  <ellipse cx="250" cy="250" rx="20" ry="4" fill="none" stroke="#ffffff" stroke-width="1" opacity="0.3"/>
  
  <!-- Birds -->
  <path d="M80 80 Q85 75 90 80 Q95 75 100 80" stroke="#333" stroke-width="2" fill="none" opacity="0.6"/>
  <path d="M320 70 Q325 65 330 70 Q335 65 340 70" stroke="#333" stroke-width="2" fill="none" opacity="0.4"/>
  
  <!-- Title -->
  <text x="200" y="35" text-anchor="middle" fill="#2c5530" font-family="Arial, sans-serif" font-size="22" font-weight="bold" opacity="0.9">
    자연의 리듬
  </text>
  
  <!-- Artist signature -->
  <text x="360" y="280" text-anchor="end" fill="#2c5530" font-family="Arial, sans-serif" font-size="12" opacity="0.7">
    한자연
  </text>
</svg>