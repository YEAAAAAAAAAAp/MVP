<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg6" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#667eea;stop-opacity:1" />
      <stop offset="33%" style="stop-color:#764ba2;stop-opacity:1" />
      <stop offset="66%" style="stop-color:#f093fb;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f5576c;stop-opacity:1" />
    </linearGradient>
    <pattern id="modern" x="0" y="0" width="40" height="40" patternUnits="userSpaceOnUse">
      <rect x="0" y="0" width="20" height="20" fill="#fff" opacity="0.1"/>
      <rect x="20" y="20" width="20" height="20" fill="#fff" opacity="0.1"/>
    </pattern>
  </defs>
  
  <!-- Background -->
  <rect width="400" height="300" fill="url(#bg6)"/>
  
  <!-- Pattern overlay -->
  <rect width="400" height="300" fill="url(#modern)"/>
  
  <!-- Modern architectural elements -->
  <rect x="80" y="100" width="60" height="120" fill="#fff" opacity="0.8" rx="5"/>
  <rect x="90" y="110" width="15" height="15" fill="#667eea" opacity="0.7"/>
  <rect x="110" y="110" width="15" height="15" fill="#f5576c" opacity="0.7"/>
  <rect x="90" y="130" width="15" height="15" fill="#f093fb" opacity="0.7"/>
  <rect x="110" y="130" width="15" height="15" fill="#764ba2" opacity="0.7"/>
  
  <!-- Curved modern elements -->
  <circle cx="250" cy="150" r="40" fill="none" stroke="#fff" stroke-width="4" opacity="0.6"/>
  <circle cx="250" cy="150" r="25" fill="#fff" opacity="0.3"/>
  
  <!-- Geometric triangular installation -->
  <polygon points="300,80 350,160 250,160" fill="#fff" opacity="0.5"/>
  <polygon points="310,100 340,140 280,140" fill="none" stroke="#fff" stroke-width="2" opacity="0.7"/>
  
  <!-- Contemporary lines -->
  <line x1="50" y1="200" x2="150" y2="100" stroke="#fff" stroke-width="3" opacity="0.6"/>
  <line x1="350" y1="200" x2="250" y2="100" stroke="#fff" stroke-width="3" opacity="0.6"/>
  
  <!-- Floating elements -->
  <rect x="180" y="80" width="8" height="8" fill="#fff" opacity="0.8" transform="rotate(45 184 84)"/>
  <rect x="320" y="200" width="8" height="8" fill="#fff" opacity="0.7" transform="rotate(45 324 204)"/>
  <rect x="60" y="160" width="8" height="8" fill="#fff" opacity="0.9" transform="rotate(45 64 164)"/>
  
  <!-- Contemporary art frame -->
  <rect x="40" y="40" width="80" height="60" fill="none" stroke="#fff" stroke-width="2" opacity="0.8" rx="3"/>
  <rect x="45" y="45" width="70" height="50" fill="#fff" opacity="0.2"/>
  
  <!-- Title -->
  <text x="200" y="30" text-anchor="middle" fill="#fff" font-family="Arial, sans-serif" font-size="20" font-weight="bold" opacity="0.9">
    현대의 해석
  </text>
  
  <!-- Artist signature -->
  <text x="360" y="280" text-anchor="end" fill="#fff" font-family="Arial, sans-serif" font-size="12" opacity="0.7">
    최현대
  </text>
</svg>