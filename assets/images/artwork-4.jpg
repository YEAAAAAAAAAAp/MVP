<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg4" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2c3e50;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#34495e;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2c3e50;stop-opacity:1" />
    </linearGradient>
    <pattern id="grid" x="0" y="0" width="20" height="20" patternUnits="userSpaceOnUse">
      <path d="M 20 0 L 0 0 0 20" fill="none" stroke="#3498db" stroke-width="1" opacity="0.3"/>
    </pattern>
  </defs>
  
  <!-- Background -->
  <rect width="400" height="300" fill="url(#bg4)"/>
  
  <!-- Grid pattern -->
  <rect width="400" height="300" fill="url(#grid)"/>
  
  <!-- Digital elements - circuit board style -->
  <circle cx="100" cy="80" r="20" fill="none" stroke="#3498db" stroke-width="2" opacity="0.8"/>
  <circle cx="300" cy="120" r="15" fill="#3498db" opacity="0.6"/>
  <rect x="180" y="60" width="40" height="40" fill="none" stroke="#e74c3c" stroke-width="2" opacity="0.7" rx="5"/>
  <circle cx="250" cy="200" r="25" fill="none" stroke="#f39c12" stroke-width="3" opacity="0.8"/>
  
  <!-- Connection lines -->
  <line x1="120" y1="80" x2="180" y2="80" stroke="#3498db" stroke-width="2" opacity="0.6"/>
  <line x1="220" y1="80" x2="285" y2="120" stroke="#e74c3c" stroke-width="2" opacity="0.6"/>
  <line x1="200" y1="100" x2="250" y2="175" stroke="#f39c12" stroke-width="2" opacity="0.6"/>
  
  <!-- Digital pixels/squares -->
  <rect x="50" y="150" width="8" height="8" fill="#e74c3c" opacity="0.8"/>
  <rect x="65" y="150" width="8" height="8" fill="#3498db" opacity="0.8"/>
  <rect x="80" y="150" width="8" height="8" fill="#f39c12" opacity="0.8"/>
  <rect x="95" y="150" width="8" height="8" fill="#2ecc71" opacity="0.8"/>
  
  <rect x="50" y="165" width="8" height="8" fill="#9b59b6" opacity="0.8"/>
  <rect x="65" y="165" width="8" height="8" fill="#e67e22" opacity="0.8"/>
  <rect x="80" y="165" width="8" height="8" fill="#1abc9c" opacity="0.8"/>
  <rect x="95" y="165" width="8" height="8" fill="#f1c40f" opacity="0.8"/>
  
  <!-- Building silhouettes -->
  <rect x="320" y="180" width="15" height="60" fill="#ecf0f1" opacity="0.4"/>
  <rect x="340" y="160" width="12" height="80" fill="#ecf0f1" opacity="0.5"/>
  <rect x="355" y="170" width="10" height="70" fill="#ecf0f1" opacity="0.3"/>
  <rect x="370" y="150" width="20" height="90" fill="#ecf0f1" opacity="0.6"/>
  
  <!-- Title -->
  <text x="200" y="40" text-anchor="middle" fill="#3498db" font-family="Arial, sans-serif" font-size="20" font-weight="bold" opacity="0.9">
    도시의 흔적
  </text>
  
  <!-- Artist signature -->
  <text x="360" y="280" text-anchor="end" fill="#3498db" font-family="Arial, sans-serif" font-size="12" opacity="0.7">
    정도시
  </text>
</svg>